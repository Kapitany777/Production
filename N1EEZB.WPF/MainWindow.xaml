<Window x:Class="N1EEZB.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:N1EEZB.WPF"
        mc:Ignorable="d"
        Title="Production Module" Height="350" Width="525"
        Style="{StaticResource WindowMain}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
            <RowDefinition />
        </Grid.RowDefinitions>

        <Menu Grid.Row="0">
            <MenuItem Header="File">
                <MenuItem Header="Exit program" />
            </MenuItem>

            <MenuItem Header="Help">
                <MenuItem Header="About Production Module" />
            </MenuItem>
        </Menu>
    </Grid>
</Window>
